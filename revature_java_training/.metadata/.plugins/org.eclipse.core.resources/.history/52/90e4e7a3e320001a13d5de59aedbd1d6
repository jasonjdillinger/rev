package mypack;

public class Hello {

	public static void main(String[] args) {
		System.out.println("Hello");
		iAmStatic();
		Demo.hey();
		
		Hello h=new Hello();
		h.iamNonStatic();
		h.iAmStatic(); //not recommended
		
		Demo d=new Demo();
		d.demoHello();
		
		new Demo().demoHello();
	}
	
	public static void iAmStatic() {
		System.out.println("Static method of Hello class");
	}
	
	public void iamNonStatic() {
		System.out.println("Non static method of Hello class");
	}

}
